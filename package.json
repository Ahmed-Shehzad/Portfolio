{
  "name": "portfolio",
  "version": "1.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run build &&npm run worker:watch & next dev",
    "build": "npm run build:worker && next build",
    "prebuild": "npm run format:fix",
    "start": "next start",
    "lint": "eslint . --fix",
    "build:worker": "tsc -p tsconfig.worker.json",
    "worker:watch": "tsc -p tsconfig.worker.json -w",
    "type-check": "tsc --noEmit --incremental --tsBuildInfoFile node_modules/.cache/tsconfig.tsbuildinfo",
    "type-check:ci": "tsc --noEmit --project tsconfig.json --listFiles --diagnostics",
    "type-check:strict": "tsc --noEmit --strict --exactOptionalPropertyTypes --noUncheckedIndexedAccess",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "test:ci": "CI=1 vitest run --coverage && node scripts/convert-junit-to-generic.mjs coverage/sonar-report.xml coverage/generic-report.xml",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write --list-different .",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "check": "npm run format:check && npm run lint && npm run type-check",
    "validate": "npm run type-check && npm run format:check && npm run lint && npm run build",
    "fix": "npm run format:fix && npm run lint:fix",
    "security:check": "node scripts/security-check.mjs",
    "security:audit": "npm audit --audit-level=moderate",
    "url:check": "node scripts/check-url-consistency.mjs",
    "vercel-build": "npm run build",
    "vercel:build": "vercel build",
    "vercel:build:prod": "vercel build --prod",
    "vercel:deploy": "vercel deploy --prebuilt",
    "vercel:deploy:prod": "vercel deploy --prebuilt --prod",
    "deploy:preview": "npm run vercel:build && npm run vercel:deploy",
    "deploy:production": "npm run vercel:build:prod && npm run vercel:deploy:prod",
    "sonar:up": "docker compose -f docker-compose.sonar.yml up -d --wait",
    "sonar:down": "docker compose -f docker-compose.sonar.yml down",
    "sonar:scan": "SONAR_TOKEN=$SONAR_TOKEN docker compose -f docker-compose.sonar.yml run --rm sonar-scanner",
    "prepare": "husky"
  },
  "dependencies": {
    "@sparticuz/chromium": "^143.0.4",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/react-query": "^5.90.16",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@types/nodemailer": "^7.0.4",
    "@vercel/analytics": "^1.6.1",
    "@vercel/speed-insights": "^1.3.1",
    "browserslist": "^4.28.1",
    "leaflet": "^1.9.4",
    "next": "^16.1.1",
    "next-intl": "^4.7.0",
    "nodemailer": "7.0.12",
    "puppeteer": "^24.34.0",
    "puppeteer-core": "^24.34.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-leaflet": "^5.0.0",
    "sharp": "^0.34.5",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@next/bundle-analyzer": "^16.1.1",
    "@next/eslint-plugin-next": "^16.1.1",
    "@react-leaflet/core": "^3.0.0",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^25.0.3",
    "@types/puppeteer": "^7.0.4",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/xml2js": "0.4.14",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-sonarjs": "^3.0.5",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16",
    "xml2js": "^0.6.2"
  },
  "pnpm": {
    "patchedDependencies": {}
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,html,css,scss}": [
      "prettier --write"
    ]
  }
}
